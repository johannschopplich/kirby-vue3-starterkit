{
  "name": "johannschopplich/kirby-helpers",
  "description": "Dotenv support, meta tags generation and more for the Kirby CMS",
  "type": "kirby-plugin",
  "version": "4.0.0",
  "keywords": [
    "getkirby",
    "kirby",
    "kirby-cms",
    "env",
    "dotenv",
    "seo",
    "meta",
    "meta-tags",
    "vite"
  ],
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/kirby-helpers#readme",
  "authors": [
    {
      "name": "Johann Schopplich",
      "email": "pkg@johannschopplich.com",
      "homepage": "https://johannschopplich.com"
    }
  ],
  "require": {
    "getkirby/composer-installer": "^1.2",
    "vlucas/phpdotenv": "^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "@stable",
    "getkirby/cms": "^3.9"
  },
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "JohannSchopplich\\": "classes/JohannSchopplich/"
    }
  },
  "scripts": {
    "fix": "php-cs-fixer fix",
    "dist": "composer install --no-dev --optimize-autoloader"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false
  }
}
